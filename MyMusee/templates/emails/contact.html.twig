
<div class="container">

    <h3>Bonjour !</h3>
    <div>
        <h6>Nom - Prénom</h6>
        <div class="row">
            <div class="col">{{ contact.lastname}}</div>
            <div class="col">{{ contact.firstname}}</div>
        </div>
    </div>
    <div>
        <h6>Email*</h6>
        <div>
            {{ contact.email}}
        </div>
    </div>
    <div>
        <h6>Téléphone*</h6>
        <div>
            {{ contact.phone}}
        </div>
    </div>
    <div>
        <h6>Sujet du message*</h6>
        <div>
            {{ contact.messagesubject}}
        </div>
    </div>
    <div>
        <h6>Votre message*</h6>
        <div>
            {{ contact.message}}
        </div>
    </div>
</div>